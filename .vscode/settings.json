{
  "[typescript]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[javascript]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src/renderer/src",
    "@c": "${workspaceRoot}/src/renderer/src/controller",
    "@h": "${workspaceRoot}/src/renderer/src/hook",
    "@l": "${workspaceRoot}/src/renderer/src/layout",
    "@s": "${workspaceRoot}/src/renderer/src/store",
    "@p": "${workspaceRoot}/src/renderer/src/views",
    "@a": "${workspaceRoot}/src/renderer/src/api",
  },
}